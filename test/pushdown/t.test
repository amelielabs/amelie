open E0 "uuid": "00000000-0000-0000-0000-000000000000", "listen": [{ "host": "127.0.0.1", "port": 3485 }], "backends": 1
connect E0 S0 localhost:3485

# test: select from table group by (large table)
create table test (id int primary key serial)
insert into test generate 10000
explain select * from (select count(*) from test group by id) t
select * from (select count(*) from test group by id) t

disconnect S0
close E0
