#

open E0 "uuid": "00000000-0000-0000-0000-000000000000", "listen": [{ "host": "127.0.0.1", "port": 3485 }]
connect E0 S0 127.0.0.1:3485

# test: uuid create
select uuid "00000000-0000-0000-0000-000000000001"
select uuid "00000000-0000-0000-0000-000000000001"::type
select "00000000-0000-0000-0000-000000000001"::uuid
select "00000000-0000-0000-0000-000000000001"::uuid::type

# test: uuid invalid
select uuid "00000000-0000-0000-0000-00000000000_"

# test: uuid = uuid
select "00000000-0000-0000-0000-000000000001"::uuid = "00000000-0000-0000-0000-000000000001"::uuid
select "00000000-0000-0000-0000-000000000001"::uuid = uuid "00000000-0000-0000-0000-000000000000"

# test: uuid = null
select "00000000-0000-0000-0000-000000000001"::uuid = null

# test: uuid = unsupported
select "00000000-0000-0000-0000-000000000001"::uuid = ""

# test: uuid <> uuid
select "00000000-0000-0000-0000-000000000001"::uuid <> "00000000-0000-0000-0000-000000000001"::uuid
select "00000000-0000-0000-0000-000000000001"::uuid <> uuid "00000000-0000-0000-0000-000000000000"

# test: uuid >= uuid
select "00000000-0000-0000-0000-000000000001"::uuid >= "00000000-0000-0000-0000-000000000000"::uuid
select "00000000-0000-0000-0000-000000000001"::uuid >= "00000000-0000-0000-0000-000000000001"::uuid

# test: uuid >= unsupported
select "00000000-0000-0000-0000-000000000001"::uuid >= 1

# test: uuid > uuid
select "00000000-0000-0000-0000-000000000001"::uuid > "00000000-0000-0000-0000-000000000000"::uuid
select "00000000-0000-0000-0000-000000000001"::uuid > "00000000-0000-0000-0000-000000000001"::uuid

# test: uuid > unsupported
select "00000000-0000-0000-0000-000000000001"::uuid > 1

# test: uuid <= uuid
select "00000000-0000-0000-0000-000000000001"::uuid <= "00000000-0000-0000-0000-000000000000"::uuid
select "00000000-0000-0000-0000-000000000001"::uuid <= "00000000-0000-0000-0000-000000000001"::uuid

# test: uuid <= unsupported
select "00000000-0000-0000-0000-000000000001"::uuid <= 1

# test: uuid < uuid
select "00000000-0000-0000-0000-000000000001"::uuid < "00000000-0000-0000-0000-000000000000"::uuid
select "00000000-0000-0000-0000-000000000001"::uuid < "00000000-0000-0000-0000-000000000001"::uuid

# test: uuid < unsupported
select "00000000-0000-0000-0000-000000000001"::uuid < 1

disconnect S0
close E0
