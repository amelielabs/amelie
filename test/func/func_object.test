# test: has update
create table test (id int primary key, data object)
insert into test values (0, {"a": 1, "b": 2, "c": 3})
insert into test values (1, {"a": 1, "b": 2})
insert into test values (2, {"a": 1, "c": 3})
select * from test where data::has("b")
update test set data = data::set("c", 3) where not data::has("c")
select * from test
delete from test where data::has("b") returning *
select * from test
drop table test

