{"code": 1, "msg": "CREATE USER PASSWORD <value> string expected"}
create user test2 password "test" abc
{"code": 1, "msg": "unknown command: <abc>"}
create user test2 password "test"
[]
show users
[{
  "name": "test"
}, {
  "name": "test2"
}]
select name from system.users() where name = "test2"
["test2"]
